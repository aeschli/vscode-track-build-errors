{
	"name": "vscode-track-build-errors",
	"private": true,
	"publisher": "aeschli",
	"displayName": "VSCode Build Log Tracker",
	"description": "Tracks build errors generated by VSCode's gulp watch",
	"icon": "resources/vscode.png",
	"license": "SEE LICENSE IN LICENSE.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/aeschli/vscode-track-build-errors"
	},
	"bugs": {
		"url": "https://github.com/aeschli/vscode-track-build-errors/issues"
	},
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:.build/log"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0"
	}
}
